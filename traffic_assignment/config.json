{
    "data": {
        "input_file": "Scats Data October 2006.xls",
        "sequence_length": 24,
        "test_size": 0.2,
        "validation_size": 0.1
    },
    "model": {
        "lstm": {
            "units": 50,
            "dropout": 0.2,
            "learning_rate": 0.001,
            "batch_size": 32,
            "epochs": 50
        },
        "gru": {
            "units": 50,
            "dropout": 0.2,
            "learning_rate": 0.001,
            "batch_size": 32,
            "epochs": 50
        }
    },
    "gui": {
        "window_size": "1200x800",
        "map_center": [-37.8136, 144.9631],
        "map_zoom": 12
    },
    "logging": {
        "level": "INFO",
        "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
} 